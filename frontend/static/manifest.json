{
    "manifest_version": 3,
    "name": "copium.dev",
    "version": "1.0.0",
    "description": "Add Applications to your copium.dev dashboard",
    "action": {
        "default_title": "copium.dev",
        "default_popup": "index.html",
        "default_icon": {
            "16": "favicon.png",
            "48": "favicon.png",
            "128": "favicon.png"
        }
    },
    "permissions": [
        "activeTab",
        "scripting"
    ]
}